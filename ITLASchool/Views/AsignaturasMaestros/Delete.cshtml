@model ITLASchool.Models.AsignaturasMaestros

@{
    ViewData["Title"] = "Delete";
}


<br />
<h3>¿Esta seguro de eliminar esta asignacion?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Asignaturas)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Asignaturas.Nombre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Profesores)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Profesores.Apellido)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="AsignaturasMaestrosID" />
        <a type="button" class="btn btn-success" asp-action="MenuAsignTeachers">Cancelar</a> |
        <input type="submit" value="Eliminar" class="btn btn-danger" />

    </form>
</div>

